// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Veiculos {
  id Int @id @default(autoincrement())
  model String
  color String
  price Int
  description String
  status Boolean @default(false)
}